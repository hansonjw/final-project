\n                  <Footer />\n                </div>\n              </Router>\n      </ApolloProvider>\n    </ChakraProvider>\n  );\n}\n\nexport default App;\n\n","/Users/justinhanson/prepos/final-project/client/src/utils/auth.js",[],"/Users/justinhanson/prepos/final-project/client/src/pages/Home.js",["104","105","106"],"import React from 'react';\nimport { Spacer, SimpleGrid, Center, Button, Stack, Text, Box } from '@chakra-ui/react';\n\nimport Auth from '../utils/auth';\n\nimport StockQuery from '../components/StockQuery';\n\n \nconst Home = () => {\n\n  const loggedIn = Auth.loggedIn();\n\n  return (\n    <div class=\"body\">\n\n      <div>\n        {loggedIn ? (\n          <div>\n            <StockQuery></StockQuery>\n          </div>\n        ):(\n          <Center>\n          <Stack spacing={10}>\n              <Spacer />\n              <Center>\n                <Text  fontSize=\"3xl\" color='white'>Welcome to Otium Capital</Text>\n              </Center>\n              <Center>\n                <Text fontSize=\"m\" color='white'>Please login or signup to begin</Text>\n              </Center>\n          </Stack>\n        </Center>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Home","/Users/justinhanson/prepos/final-project/client/src/components/NavBar.js",["107","108","109"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport Auth from '../utils/auth';\nimport { Box, Spacer, Heading, Flex, Center, Text, Button } from '@chakra-ui/react';\n\nconst Header = () => {\n  const logout = event => {\n    event.preventDefault();\n    Auth.logout();\n  };\n\n  return (\n    // <header class=\"header\">\n    <div class=\"header\">\n      <Flex\n        as=\"nav\"\n        align=\"center\"\n        justify=\"space-between\"\n        wrap=\"wrap\"\n        w=\"100%\"\n        padding=\".5rem\"\n        // bg=\"red.900\"\n        bg=\"#1a0a00\"\n        color=\"white\"\n      >\n        <Heading as=\"h4\" size=\"md\" w=\"200\" 